{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "plugin:react/recommended", "plugin:prettier/recommended"
  ],
  "globals": {
    "graphql": true,
    "__PATH_PREFIX__": true,
    "fetch": true,
    "sessionStorage": true,
    "localStorage": true,
    "alert": true,
    "gtag": true
  },
  "rules": {
    "no-unused-vars": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
